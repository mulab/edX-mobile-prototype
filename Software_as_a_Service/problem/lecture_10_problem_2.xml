<problem display_name="Lecture 10 Problem 2: Unobtrusive JavaScript">
  <startouttext/>
  <p>Using unobtrusive vs. traditional (obtrusive) JS improves compliance with which SOLID principle?</p>
  <endouttext/>
  <multiplechoiceresponse direction="vertical" randomize="yes">
    <choicegroup type="MultipleChoice">
      <choice correct="false">
        <text>
          <span style="color: #F90;">Single Responsibility principle</span>
        </text>
      </choice>
      <choice correct="true">
        <text>
          <span style="color: #063;">Open/Closed principle</span>
        </text>
      </choice>
      <choice correct="false">
        <text>
          <span style="color: #C00;">Demeter principle</span>
        </text>
      </choice>
      <choice correct="false">
        <text>
          <span style="color:#006;">Dependency Inversion/Injection principle</span>
        </text>
      </choice>
    </choicegroup>
  </multiplechoiceresponse>
</problem>
