  <choiceresponse>
    <radiogroup>
      <choice correct="false">
        <text>
          <code>result.should_not be_empty</code>
        </text>
      </choice>
      <choice correct="true">
        <text>
          <code>5.should be &lt;=&gt; result</code>
        </text>
      </choice>
      <choice correct="false">
        <text>
          <code>result.should_not match /^D'oh!$/ </code>
        </text>
      </choice>
