    <choicegroup type="MultipleChoice">
      <choice correct="false">
        <text>
          <span style="color: #F90;">Show has many Vouchers, through Showdate</span>
        </text>
      </choice>
      <choice correct="true">
        <text>
          <span style="color: #063;">Item belongs to Vouchertype</span>
        </text>
      </choice>
      <choice correct="false">
        <text>
          <span style="color: #C00;">Customer has many Donations</span>
        </text>
      </choice>
      <choice correct="false">
        <text>
          <span style="color:#006;">Voucher belongs to Vouchertype</span>
        </text>
      </choice>
    </choicegroup>
  </multiplechoiceresponse>
</problem>
